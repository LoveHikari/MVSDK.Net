<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>MVSDK_Net.Standard</PackageId>
		<Description>大华工业相机SDK</Description>
		<Version>2.4.2.3</Version>
		<Company>5cm R &amp; D department</Company>
		<Authors>Asanoame</Authors>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<PackageReleaseNotes>
			大华工业相机SDK2.4.2.3版本
		</PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/LoveHikari/MVSDK.Net</PackageProjectUrl>
		<PackageTags>common</PackageTags>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Copyright>Copyright (c) Hikari.</Copyright>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>
	
	<ItemGroup>
		<None Include="../README.md" pack="true" PackagePath="." />
	</ItemGroup>
</Project>